{
  "name": "timongqlserver",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/server.js",
  "scripts": {
    "build": "webpack -d",

    "pre-build": "babel ./src ./schemas -d lib --source-maps",
    "serve": "yarn pre-build && nodemon --experimental-worker --inspect-brk",

    "start": "nodemon ./src/server.js --exec babel-node --presets es2015 --plugins transform-class-properties",
    "debug": "nodemon ./src/server.js --exec babel-node --presets es2015 --plugins transform-class-properties --inspect-brk",
    "flow": "flow",
    "test": "jest",
    "create-snaps-index": "babel-node ./elasticsearch/snapsMappings.js",
    "load-mock-snaps": "babel-node ./elasticsearch/load_snaps.js",
    "loadSnapsBulk": "babel-node --max_old_space_size=8192 ./data/loadSnapsBulk.js -t ES,mySQL",
    "loadSnapsIntervals": "babel-node ./data/loadSnapsIntervals.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.{js,jsx}"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.1.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "^6.26.0",
    "base-64": "^0.1.0",
    "casual": "^1.5.19",
    "elastic-builder": "^2.0.2",
    "elasticsearch": "^15.1.1",
    "express": "^4.16.4",
    "fast-csv": "^2.4.1",
    "graphql": "^14.0.2",
    "lodash.assign": "^4.2.0",
    "moment": "^2.22.2",
    "moment-random": "^1.0.4",
    "mssql": "^4.2.1",
    "mysql": "^2.16.0",
    "nodemon": "^1.17.3",
    "optimist": "^0.6.1",
    "promise-mysql": "^3.3.1",
    "rxjs": "^6.3.3",
    "stream-to-promise": "^2.2.0",
    "webpack": "4.6.0",
    "webworker-threads": "^0.7.17"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^22.4.3",
    "webpack-cli": "^3.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "flow",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-class-properties",
        {
          "spec": true
        }
      ]
    ]
  }
}
